{
  "name": "koishi-plugin-steam-family-bot",
  "description": "一个用于新版 Steam 家庭的库存监控 koishi Bot 插件",
  "version": "0.0.5-dev.4",
  "main": "lib/koishi/index.js",
  "typings": "lib/koishi/index.d.ts",
  "files": [
    "lib",
    "dist"
  ],
  "license": "MIT",
  "author": {
    "name": "ktKongTong",
    "email": "kt@ktlab.io"
  },
  "repository": "https://github.com/ktKongTong/koishi-steam-family-bot",
  "homepage": "https://koishi-steam-family-bot.vercel.app/",
  "keywords": [
    "steam",
    "steam-family",
    "koishi",
    "koishi-plugin"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "koishi": {
    "preview": true,
    "service": {
      "required": [
        "puppeteer",
        "database",
        "cron"
      ]
    },
    "locales": [
      "en",
      "zh"
    ],
    "description": {
      "en": "a koishi bot plugin to monitor steam family library changes",
      "zh": "用于新版 Steam 家庭的库存监控插件"
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.3.0",
    "@koishijs/plugin-mock": "^2.6.5",
    "@types/chai": "^4.3.16",
    "@types/js-yaml": "^4.0.9",
    "@types/mocha": "^10.0.6",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "chai": "^5.1.1",
    "eslint": "^9.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.3.0",
    "globby": "^14.0.1",
    "husky": "^9.0.11",
    "js-yaml": "^4.1.0",
    "lint-staged": "^15.2.5",
    "mocha": "^10.4.0",
    "react-jsx": "^1.0.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.0.0-alpha.24",
    "vitepress": "^1.2.2"
  },
  "peerDependencies": {
    "canvas": "^2.11.2",
    "dayjs": "^1.11.11",
    "echarts": "^5.5.0",
    "jwt-decode": "^4.0.0",
    "koishi": "^4.17.4",
    "koishi-plugin-puppeteer": "^3.8.4",
    "lodash": "^4.17.21",
    "node-steam-family-group-api": "^0.0.4-dev.patch04",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "steam-session": "^1.7.2",
    "steamid": "^2.0.0"
  },
  "scripts": {
    "test": "ts-node ./tests/index.spec.ts",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "lint": "eslint src",
    "lint-fix": "eslint --fix src",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "clean": "rm -rf lib  tsconfig.tsbuildinfo",
    "copy-yml-resource": "node ./scripts/build.mjs",
    "compile": "tsc",
    "build": "npm run clean && npm run copy-yml-resource && npm run compile",
    "pub": "npm run build && npm publish"
  },
  "lint-staged": {
    "*.{js,ts,vue,jsx,tsx}": [
      "eslint --cache --fix src"
    ]
  }
}
